{
  "root": "packages/nodejs",
  "sourceRoot": "packages/nodejs/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["packages/nodejs/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/nodejs"],
      "options": {
        "jestConfig": "packages/nodejs/jest.config.js",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/nodejs",
        "tsConfig": "packages/nodejs/tsconfig.lib.json",
        "packageJson": "packages/nodejs/package.json",
        "main": "packages/nodejs/src/index.ts",
        "assets": ["packages/nodejs/*.md"]
      }
    }
  },
  "tags": []
}
