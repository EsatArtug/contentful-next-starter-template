{
  "root": "packages/test/gatsby",
  "sourceRoot": "packages/test/gatsby/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/gatsby:build",
      "options": {
        "outputPath": "packages/test/gatsby/public",
        "uglify": true,
        "color": true,
        "profile": false
      },
      "configurations": {
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/gatsby:server",
      "options": {
        "buildTarget": "test-gatsby:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "test-gatsby:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/test/gatsby/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/test/gatsby"],
      "options": {
        "jestConfig": "packages/test/gatsby/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
