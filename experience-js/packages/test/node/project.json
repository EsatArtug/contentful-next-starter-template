{
  "root": "packages/test/node",
  "sourceRoot": "packages/test/node/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/test/node",
        "main": "packages/test/node/src/main.ts",
        "tsConfig": "packages/test/node/tsconfig.app.json",
        "assets": ["packages/test/node/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "packages/test/node/src/environments/environment.ts",
              "with": "packages/test/node/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "test-node:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/test/node/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/test/node"],
      "options": {
        "jestConfig": "packages/test/node/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
