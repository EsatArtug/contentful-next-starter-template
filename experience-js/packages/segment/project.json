{
  "root": "packages/segment",
  "sourceRoot": "packages/segment/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/segment",
        "tsConfig": "packages/segment/tsconfig.lib.json",
        "project": "packages/segment/package.json",
        "entryFile": "packages/segment/src/index.ts",
        "external": [],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "assets": [
          {
            "glob": "packages/segment/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["packages/segment/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/segment"],
      "options": {
        "jestConfig": "packages/segment/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
